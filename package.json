{
    "name": "angularjs-vertex",
    "version": "1.0.0",
    "description": "seed project for angularJS",
    "scripts": {
        "build": "webpack --bail --progress --profile --config ./config/webpack.dev.config.js",
        "build:prod": "webpack --bail --progress --profile --config ./config/webpack.prod.config.js",
        "start": "webpack-dev-server --history-api-fallback --inline --progress --config ./config/webpack.dev.config.js",
        "start:prod": "webpack-dev-server --history-api-fallback --inline --progress --config ./config/webpack.prod.config.js",
        "test": "karma start ./config/karma.config.js",
        "webdriver-manager": "webdriver-manager",
        "test:e2e": "protractor ./config/protractor.config.js"
    },
    "author": "Brittany Moore",
    "license": "MIT",
    "dependencies": {
        "angular": "1.6.3",
        "angular-ui-router": "1.0.0-rc.1",
        "angular-mocks": "1.6.3"
    },
    "devDependencies": {
        "webpack": "2.3.2",
        "webpack-dev-server": "2.4.2",
        "webpack-merge": "4.1.0",
        "html-webpack-plugin": "2.28.0",
        "raw-loader": "0.5.1",
        "babel-core": "6.24.0",
        "babel-preset-es2015": "6.24.0",
        "babel-loader": "6.4.1",
        "css-loader": "0.27.3",
        "style-loader": "0.16.0",
        "jasmine-core": "2.5.2",
        "karma": "1.5.0",
        "karma-chrome-launcher": "2.0.0",
        "karma-jasmine": "1.1.0",
        "karma-sourcemap-loader": "0.3.7",
        "karma-spec-reporter": "0.0.30",
        "karma-webpack": "2.0.3",
        "protractor": "5.1.1",
        "jasmine-spec-reporter": "3.2.0"           
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/brittanymoore/angularjs-vertex.git"
    }
}