{
    "name": "vertex-angularjs",
    "version": "0.0.1",
    "description": "seed project for angularJS",
    "scripts": {
        "build": "webpack --bail --progress --profile --config ./config/webpack.dev.config.js",
        "build:prod": "webpack --bail --progress --profile --config ./config/webpack.prod.config.js",
        "start": "webpack-dev-server --history-api-fallback --inline --progress --config ./config/webpack.dev.config.js",
        "start:prod": "webpack-dev-server --history-api-fallback --inline --progress --config ./config/webpack.prod.config.js",
        "test": "karma start ./config/karma.conf.js",
        "webdriver-manager": "webdriver-manager",
        "webdriver:update": "webdriver-manager update",
        "test:e2e": "protractor ./config/protractor.conf.js"
    },
    "author": "Brittany Moore",
    "license": "MIT",
    "dependencies": {
        "angular": "^1.6.1",
        "angular-ui-router": "^0.3.2",
        "angular-mocks": "^1.6.1"
    },
    "devDependencies": {
        "webpack": "^1.14.0",
        "webpack-dev-server": "^1.16.2",
        "html-webpack-plugin": "^2.26.0",
        "raw-loader": "^0.5.1",
        "babel-core": "^6.21.0",
        "babel-preset-es2015": "^6.18.0",
        "babel-loader": "^6.2.10",
        "css-loader": "^0.23.0",
        "style-loader": "^0.13.0",
        "jasmine-core": "^2.5.2",
        "karma": "^1.4.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-jasmine": "^1.1.0",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-spec-reporter": "^0.0.26",
        "karma-webpack": "^2.0.1",
        "protractor": "^4.0.10"
    },
    "babel": {
        "presets": ["es2015"]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/brittanymoore/vertex-angularjs.git"
    }
}