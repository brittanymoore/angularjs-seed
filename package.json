{
    "name": "angularjs-vertex",
    "version": "1.0.0",
    "description": "seed project for angularJS",
    "license": "MIT",
    "author": "Brittany Moore <brittanymoore>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/brittanymoore/vertex-angularjs.git"
    },
    "scripts": {
        "build": "webpack --env=dev --progress --profile --colors",
        "build:prod": "webpack --env=prod --progress --profile --colors",
        "lint": "eslint src/**/*.js -c ./config/.eslintrc --ignore-path ./config/.eslintignore",
        "start": "webpack-dev-server --env=dev --open --progress",
        "start:prod": "webpack-dev-server --env=prod --open --progress",
        "test": "karma start ./config/karma.conf.js",
        "test:e2e": "protractor ./config/protractor.conf.js",
        "webdriver-manager": "webdriver-manager"
    },
    "dependencies": {
        "@uirouter/angularjs": "1.0.6",
        "angular": "1.6.5",
        "angular-mocks": "1.6.5"
    },
    "devDependencies": {
        "babel-core": "6.25.0",
        "babel-eslint": "7.2.3",
        "babel-loader": "7.1.1",
        "babel-preset-es2015": "6.24.1",
        "chunk-manifest-webpack-plugin": "1.1.2",
        "css-loader": "0.28.4",
        "eslint": "4.4.1",
        "html-webpack-plugin": "2.30.1",
        "jasmine-core": "2.7.0",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "1.7.0",
        "karma-chrome-launcher": "2.2.0",
        "karma-jasmine": "1.1.0",
        "karma-sourcemap-loader": "0.3.7",
        "karma-spec-reporter": "0.0.31",
        "karma-webpack": "2.0.4",
        "node-sass": "4.5.3",
        "postcss-loader": "2.0.6",
        "protractor": "5.1.2",
        "raw-loader": "0.5.1",
        "sass-loader": "6.0.6",
        "style-loader": "0.18.2",
        "webpack": "3.5.4",
        "webpack-chunk-hash": "0.4.0",
        "webpack-dev-server": "2.7.1",
        "webpack-merge": "4.1.0"
    }
}
